num1=int(input("Enter the first number"))
num2=int(input("Enter the second number"))
operator=input("Enterr the operator to perform the calculation")
if(operator =='+'):
   print(num1+num2)
if(operator =='-'):
   print(num1-num2)
if(operator =='*'):
   print(num1*num2)
if(operator =='/'):
   print(num1/num2)
if(operator =='%')
   print(num1%num2)
